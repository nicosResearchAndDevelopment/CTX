                           dct:description   "Das bin ich, traurig!^"@de ;
                           ctx:contentType   "image/ascii-art"^^xsd:string ;
                           ctx:contentLength "3"^^xsd:nonNegativeInteger ;
                           ctx:content       ":-("^^xsd:string ; ] ;
